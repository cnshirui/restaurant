<p id="notice"><%= notice %></p>

<p>
  <strong><%= t('label.date') %>:</strong>
  <%= @shopping.date %>
</p>

<p>
  <strong><%= t('meal') %>:</strong><br/>
  <% @shopping.shopping_meals.each do |sm| %>
    <% if sm.selected == true %>
      <%= sm.meal.date + " - " + t(sm.meal.meal_type) %><br/>
    <% end %>
  <% end %>
</p>

<%= link_to 'Edit', edit_shopping_path(@shopping) %> |
<%= link_to 'Back', shoppings_path %>
